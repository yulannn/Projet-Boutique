    <!DOCTYPE html>
    <html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Team</title>
        <link rel="stylesheet" href="/public/css/team.css">
        <link rel="stylesheet" href="/public/css/header.css">
        <link rel="stylesheet" href="/public/css/panier.css">
        <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css">
        <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
    </head>

    <body>
    <%- include('burger_menu.ejs') %>

    <%- include('panier.ejs') %>

    <main class="container">
        <%- include('header.ejs') %>

        <!--<div class="back__button">
            <a href="/teams" class="back__button__link">Retour</a>
        </div>-->

        <div class="team__display"></div>

        <div class="swiper-container">
            <div class="swiper-wrapper">

                <div class="swiper-slide">
                    <h2 id="teamPlayersText">Players</h2>
                    <div class="players__display"></div>
                </div>

                <div class="swiper-slide">
                    <h2 id="teamJerseysText">Jerseys</h2>
                    <div class="jerseys__display"></div>
                </div>

                <div id="next-button" class="swiper-button-next"></div>
                <div id="previous-button" class="swiper-button-prev"></div>

            </div>


        </div>


    </main>

    <script src="/public/js/panier.js"></script>
    <script src="/public/js/team.js"></script>
    <script src="/public/js/burger.js"></script>
    <script src="https://unpkg.com/boxicons@2.1.4/dist/boxicons.js"></script>
    </body>

    </html>